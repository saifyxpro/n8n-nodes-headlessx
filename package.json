{
  "name": "n8n-nodes-headlessx",
  "version": "1.2.2",
  "description": "n8n community node for HeadlessX API integration - web scraping, screenshots, and PDF generation",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "n8n-node",
    "headlessx",
    "web-scraping",
    "screenshot",
    "pdf",
    "puppeteer",
    "browser-automation",
    "headless-browser",
    "scraping",
    "automation"
  ],
  "license": "MIT",
  "homepage": "https://github.com/SaifyXPRO/n8n-nodes-headlessx",
  "author": {
    "name": "SaifyXPRO"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SaifyXPRO/n8n-nodes-headlessx.git"
  },
  "engines": {
    "node": ">=20.15"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npx rimraf dist && tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/HeadlessXApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/HeadlessX/HeadlessX.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "@typescript-eslint/parser": "~8.32.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.0",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "publishConfig": {
    "access": "public"
  }
}
